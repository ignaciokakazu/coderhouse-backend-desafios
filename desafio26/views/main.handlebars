                    <div class='form-group'>
                        <label>User</label>
                        <input id='userReg' class='form-control'>
                    </div>
                    <div class='form-group'>
                        <label>Password</label>
                        <input id='passwordReg' type='password' class='form-control'>
                    </div>
                    <div class='form-group'>
                        <label>Email</label>
                        <input id='email' class='form-control'>
                    </div>
                    <button id='register'>Registrarse</button>
            </div>
            <div class='col-4'>
                
            </div>
            </div>
