        <h3>Chat</h3>
            <div class="chatContainer">

                    <div id="chatDatos" class="chatDatos">
                        <div class="form-group">
                            <label>Email</label>
                            <input class="form-control" type="text" id="email">
                        </div>

                        <div class="form-group">
                            <label>Nombre</label>
                            <input class="form-control" type="text" id="name">
                        </div>

                        <div class="form-group">
                            <label>Apellido</label>
                            <input class="form-control" type="text" id="surname">
                        </div>

                        <div class="form-group">
                            <label>Edad</label>
                            <input class="form-control" type="number" id="age">
                        </div>
                        <div class="form-group">
                            <label>Alias</label>
                            <input class="form-control" type="text" id="alias">
                        </div>

                        <div class="form-group">
                            <label>Avatar</label>
                            <input class="form-control" type="text" id="avatar">
                        </div>
                        <button class="btn btn-success" onclick="iniciar();">Iniciar</button>
                    </div>
                
                
            
            
        
            <div class="chatPanel">
                <div id="chat" class="chat"></div>
            
                <div class="chatButtons">
                        <div class="form-group">
                            <input class="form-control mb-3" type="text" id="newMessage" placeholder="Escriba su mensaje"> 
                            <button id="enviar" class="btn btn-success" onclick="enviar();">Enviar</button>
                            <button id="desconectar" class="btn btn-success" onclick="desconectar();">Desconectar</button>
                        </div>
                        
                    </div>
            </div>
            
        </div>       


